{
  "name": "b2b-api",
  "description": "Chums B2B API",
  "version": "1.1.0",
  "author": "steve montgomery <steve@chums.com>",
  "main": "bundle.js",
  "private": true,
  "scripts": {
    "start": "nodemon index.js",
    "start-dev": "SET MYSQL_PORT=3307 && SET PORT=8001 && nodemon -L index.js",
    "test": "mocha --reporter-option maxDiffSize=0",
    "version": "auto-changelog -p && git add CHANGELOG.md"
  },
  "dependencies": {
    "bcryptjs": "2.x",
    "body-parser": "^1.19.0",
    "chums-local-modules": "github:UtahGooner/chums-local-modules",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "date-fns": "^2.12.0",
    "debug": "^4.1.1",
    "dotenv": "^16.0.1",
    "express": "^4.17.1",
    "helmet": "^5.0.1",
    "mysql2": "^2.1.0",
    "node-mysql": "0.4.x",
    "pug": "^3.0.2",
    "x-frame-options": "1.0.x"
  },
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/node": "^17.0.31",
    "auto-changelog": "^2.4.0",
    "b2b-types": "github:UtahGooner/b2b-types",
    "mocha": "^10.0.0",
    "nodemon": "^2.0.16",
    "typescript": "^4.7.3"
  },
  "auto-changelog": {
    "template": "keepachangelog",
    "unreleased": true,
    "commitLimit": false
  },
  "nodemonConfig": {
    "ignore": [
      "node_modules/*"
    ],
    "delay": 2500,
    "watch": [
      "src/**/*",
      "views/**/*"
    ],
    "ext": "js,mjs,json,ts"
  }
}
