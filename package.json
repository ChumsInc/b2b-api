    "cookie-parser": "^1.4.5",
    "date-fns": "^2.12.0",
    "debug": "^4.1.1",
    "dotenv": "^16.0.1",
    "express": "^4.17.1",
    "helmet": "^7.0.0",
    "mysql2": "^3.3.2",
    "node-mysql": "0.4.x",
    "pug": "^3.0.2",
    "x-frame-options": "1.0.x"
  },
  "devDependencies": {
    "@tsconfig/node18": "^18.2.0",
    "@types/express": "^4.17.11",
    "@types/node": "^20.2.3",
    "auto-changelog": "^2.4.0",
    "b2b-types": "github:UtahGooner/b2b-types",
    "mocha": "^10.0.0",
    "nodemon": "^3.0.1",
    "typescript": "^5.0.4"
  },
  "auto-changelog": {
    "template": "keepachangelog",
